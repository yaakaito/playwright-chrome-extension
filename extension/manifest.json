{
  "manifest_version": 3,
  "name": "Playwright Test Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension for Playwright testing",
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "permissions": ["tabs"],
  "icons": {
    "16": "src/icon.png",
    "48": "src/icon.png",
    "128": "src/icon.png"
  }
}